<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<Pane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.GridController">
   <children>
      <ImageView fitHeight="800.0" fitWidth="1282.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../source/GridUIMainBG.jpg" />
         </image>
      </ImageView>
      <BorderPane fx:id="borderPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="800.0" prefWidth="1200.0">
         <top>
            <Pane prefHeight="40.0" prefWidth="1200.0" BorderPane.alignment="CENTER" />
         </top>
         <left>
            <Pane prefHeight="630.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <children>
                  <ImageView fx:id="mainMenu" fitHeight="100.0" fitWidth="150.0" layoutX="25.0" layoutY="629.0" onMouseClicked="#handleMainMenu" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../source/MainMenu-Button.jpg" />
                     </image>
                  </ImageView>
               </children></Pane>
         </left>
         <right>
            <Pane prefHeight="720.0" prefWidth="280.0" BorderPane.alignment="CENTER">
               <children>
                  <Label fx:id="timer" layoutX="127.0" layoutY="52.0" prefHeight="52.0" prefWidth="163.0" text="00 : 00 :00" textAlignment="CENTER" textFill="WHITE">
                     <font>
                        <Font size="28.0" />
                     </font>
                  </Label>
                  <ImageView fx:id="done" fitHeight="100.0" fitWidth="150.0" layoutX="65.0" layoutY="425.0" onMouseClicked="#handleDone" pickOnBounds="true" preserveRatio="true">
