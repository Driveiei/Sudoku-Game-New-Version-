                        <Image url="@../source/Doner-Button.jpg" />
                     </image>
                  </ImageView>
                  <ImageView fx:id="assist" fitHeight="100.0" fitWidth="150.0" layoutX="65.0" layoutY="175.0" onMouseClicked="#handleHint" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../source/Assist-Button.jpg" />
                     </image>
                  </ImageView>
                  <ImageView fx:id="clear" fitHeight="100.0" fitWidth="150.0" layoutX="65.0" layoutY="300.0" onMouseClicked="#handleClear" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../source/Clear-Button.jpg" />
                     </image>
                  </ImageView>
                  <Label layoutX="14.0" layoutY="34.0" text="Time" textFill="WHITE">
                     <font>
                        <Font size="48.0" />
                     </font>
                  </Label>
                  <ImageView fx:id="restart" fitHeight="100.0" fitWidth="150.0" layoutX="65.0" layoutY="550.0" onMouseClicked="#handleRestart" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../source/Restart-Button.jpg" />
                     </image>
                  </ImageView>
               </children>
            </Pane>
         </right>
         <bottom>
            <Pane prefHeight="40.0" prefWidth="1200.0" BorderPane.alignment="CENTER" />
         </bottom>
      </BorderPane>
   </children>
</Pane>
